{
  "scripts": {
    "clean": "rm -rf lib_es6 lib src/docs/webpack/*.js dist",
    "build_es6": "tsc",
    "es6_to_es5": "babel lib_es6 --out-dir lib",
    "_cp_d.ts_files_shallow": "copyfiles --up 1 'lib_es6/*.d.ts' lib",
    "_cp_d.ts_files_deep": "copyfiles -u 1 'lib_es6/**/*.d.ts' lib",
    "cp_d.ts_files": "npm run _cp_d.ts_files_shallow && npm run _cp_d.ts_files_deep ",
    "build": "npm run build_es6 && npm run es6_to_es5 && npm run cp_d.ts_files",
    "prepublish": "npm run clean && npm run build",
    "compile_doc_webpack": "tsc src/docs/webpack/build.ts",
    "build_doc": "npm run compile_doc_webpack && webpack --config src/docs/webpack/build.js",
    "run_doc": "npm run compile_doc_webpack && webpack-dev-server --config src/docs/webpack/build.js",
    "storybook": "start-storybook -p 7001",
    "build-storybook": "build-storybook",
    "test": "npm run unittest",
    "unittest": "jest --config src/__unittests__/jest_unittests.json"
  },
  "name": "reactive-pixi",
  "version": "0.0.2",
  "description": "(sort-of) Reactive Pixi JS",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/react-stream-libs/reactive-pixi.git",
  "author": "devdoomari <devdoomari __at__ gmail dotttt commmm>",
  "license": "Personal",
